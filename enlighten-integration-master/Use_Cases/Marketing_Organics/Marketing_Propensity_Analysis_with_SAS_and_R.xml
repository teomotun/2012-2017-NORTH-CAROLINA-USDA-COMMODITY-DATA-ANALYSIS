<NODE CLASS="SASHELP.EMCORE.EXTENSION.CLASS" COMPONENT="EMOPEN" ID="EMOPEN2" X="858" Y="66" DESCRIPTION="Open Source - GLM" DIAGRAMID="_ROOT_" PARENTID="" TOOLTYPE="MODEL" TOOLPREFIX="EMOPEN">
<PROPERTIES Location="CATALOG" Catalog="SASHELP.EMUTIL.Em_open.SOURCE" TRAINMODE="SUPERVISED" LANGUAGE="R" R_OUTPUT="PMML" ForceRun="N" RunAction="Train" Component="EMOPEN" EDITOR="">
<VariableProperties/>
<EmTargetAttributes/>
<EmVariableAttributes/>
<EmFileProperties>
<USERTRAINCODE Type="EXTERNAL" Extension="sas">
<Data/>
</USERTRAINCODE>
<SUBMIT Type="EXTERNAL" Extension="txt">
<Data><![CDATA[
# Logistic Regression
 
library(glm2)
library(pmml)
 
&amp;EMR_MODEL &lt;- glm(&amp;EMR_CLASS_TARGET ~ &amp;EMR_CLASS_INPUT + &amp;EMR_NUM_INPUT, data= &amp;EMR_IMPORT_DATA, family= binomial())
summary(&amp;EMR_MODEL) # display results
confint(&amp;EMR_MODEL) # 95% CI for the coefficients
exp(coef(&amp;EMR_MODEL)) # exponentiated coefficients
exp(confint(&amp;EMR_MODEL)) # 95% CI for exponentiated coefficients
predict(&amp;EMR_MODEL, type=&quot;response&quot;) # predicted values
residuals(&amp;EMR_MODEL, type=&quot;deviance&quot;) # residuals
 
 
]]>
</Data>
</SUBMIT>
<EMNOTES Type="EXTERNAL" Extension="txt">
<Data/>
</EMNOTES>
</EmFileProperties>
</PROPERTIES>
