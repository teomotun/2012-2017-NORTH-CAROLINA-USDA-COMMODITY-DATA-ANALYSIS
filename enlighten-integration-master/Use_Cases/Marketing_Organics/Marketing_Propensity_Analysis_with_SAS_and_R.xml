<NODE CLASS="SASHELP.EMCORE.EXTENSION.CLASS" COMPONENT="EMOPEN" ID="EMOPEN3" X="660" Y="528" DESCRIPTION="Open Source randomForest" DIAGRAMID="_ROOT_" PARENTID="" TOOLTYPE="UTIL" TOOLPREFIX="EMOPEN">
<PROPERTIES Location="CATALOG" Catalog="Sashelp.Emutil.Em_open.SOURCE" TRAINMODE="SUPERVISED" LANGUAGE="R" R_OUTPUT="MERGE" ForceRun="N" RunAction="Train" Component="EMOPEN" EDITOR="">
